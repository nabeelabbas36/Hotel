<h1>Listing locations</h1>

<table class="table table-striped table-bordered table-condensed table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Gmaps</th>
      <th>Start time</th>
      <th>Close time</th>
      <th></th>
      <th></th>
      
    </tr>
  </thead>

  <tbody>
    <% @locations.each do |location| %>
      <tr>
        <td><%= location.name %></td>
        <td><%= location.address %></td>
        <td><%= location.longitude %></td>
        <td><%= location.latitude %></td>
        <td><%= location.gmaps %></td>
        <td><%= location.start_time %></td>
        <td><%= location.close_time %></td>
        <td><%= link_to 'Show', admins_locations_path %></td>
        <td><%= link_to 'Edit', edit_admins_location_path(location) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>